[coverage:run]
branch = True
omit =
    authlib/client/*
    authlib/flask/*
    authlib/django/*
    authlib/specs/*
    authlib/integrations/httpx_client/*
    authlib/integrations/starlette_client/*

[coverage:report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    raise DeprecationWarning
    deprecate
